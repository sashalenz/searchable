{
    "name": "sashalenz/searchable",
    "description": "This is my package searchable",
    "keywords": [
        "sashalenz",
        "laravel",
        "searchable"
    ],
    "homepage": "https://github.com/sashalenz/searchable",
    "license": "MIT",
    "authors": [
        {
            "name": "Oleksandr Petrovskyi",
            "email": "sashalenz@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^9.10",
        "illuminate/support": "^9.10",
        "spatie/laravel-query-builder": "^5.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "nunomaduro/collision": "^6.2",
        "orchestra/testbench": "^7.4",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.2",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Sashalenz\\Searchable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sashalenz\\Searchable\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
